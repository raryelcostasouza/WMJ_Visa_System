                              <Insets bottom="5.0" />
                           </GridPane.margin></Button>
                        <Button fx:id="bPreview6" layoutX="266.0" layoutY="133.0" mnemonicParsing="false" onAction="#actionPreviewAckOverstayPenalties" text="Preview" GridPane.columnIndex="1" GridPane.rowIndex="6">
