                     <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </VBox.margin>
                  </GridPane>
               </children>
            </VBox>
         </content>
    </Tab>
    <Tab text="Register Visa Extension">
         <content>
            <VBox>
               <children>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label text="Parent Visa Number:" />
                        <TextField fx:id="tfParentVisaNumber" editable="false" />
                     </children>
                  </HBox>
                  <TitledPane animated="false" collapsible="false" text="Extensions:">
                     <content>
                        <HBox alignment="CENTER">
                           <children>
                              <TableView fx:id="tvExtensions" editable="true">
                                <columns>
                                  <TableColumn editable="false" prefWidth="150.0" sortable="false" text="Extension Number" />
                                  <TableColumn editable="false" prefWidth="100.0" sortable="false" text="Expiry" />
                                    <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Remove" />
                                </columns>
                              </TableView>
                           </children>
                        </HBox>
                     </content>
                  </TitledPane>
                  <TitledPane animated="false" collapsible="false" text="Register Extension">
                     <content>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <GridPane>
                                <columnConstraints>
                                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                                  <ColumnConstraints />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <Label text="Number:" />
                                    <TextField fx:id="tfExtNumber" GridPane.columnIndex="1" />
                                    <Label text="Expiry Date:" GridPane.rowIndex="1" />
                                    <DatePicker fx:id="dpExpiryDate" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                 </children>
                              </GridPane>
                           </children>
                        </HBox>
                     </content>
                  </TitledPane>
                  <HBox alignment="CENTER">
                     <children>
                        <Button fx:id="bRegister" mnemonicParsing="false" onAction="#actionRegisterVisaExt" text="Register" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
    </Tab>
  </tabs>
</TabPane>
