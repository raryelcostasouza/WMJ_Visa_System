<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_RIGHT" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.watmarpjan.visaManager.gui.CtrPaneEditSave">
    <children>
        <Label text="Edition:">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Button fx:id="bLockUnlock" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#actionLockUnlockButton" text="Locked" />
        <Separator orientation="VERTICAL" />
        <Button fx:id="bSave" contentDisplay="CENTER" disable="true" mnemonicParsing="false" onAction="#actionSave" text="Save">
            <HBox.margin>
                <Insets left="10.0" />
            </HBox.margin>
        </Button>
    </children>
</HBox>
